.search_input {
  height: 90rpx;
  padding: 10rpx;
  /* 使用全局变量 */
  background-color: var(--themeColor);
}
.search_input navigator {
  /* 继承父元素的高度 */
  height: 100%;
  /* 文字剧中 */
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  border-radius: 15rpx;
  color: #666;
}
